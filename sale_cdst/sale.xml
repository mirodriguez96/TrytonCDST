<tryton>
    <data>

        <record model="ir.ui.view" id="sale_shop_detailed_start_cds_form">
            <field name="model">sale_shop.sale_detailed_cds.start</field>
            <field name="type">form</field>
            <field name="name">sale_shop_detailed_start_cds_form</field>
        </record>
        <record model="ir.action.wizard" id="wizard_print_sale_shop_detailed_cds">
            <field name="name">Print Cost Sale Shop Detailed Cds</field>
            <field name="wiz_name">sale_shop.sale_detailed_cds</field>
        </record>
        <record model="ir.action.report" id="report_sale_detailed_cds">
            <field name="name">Sale Cost Shop Detailed</field>
            <field name="model">sale.sale</field>
            <field name="report_name">sale_shop.report_sale_detailed_cds</field>
            <field name="report">sale_cdst/cost_sale_detailed.fods</field>
            <field name="template_extension">ods</field>
            <field name="translatable">False</field>
        </record>

        <menuitem parent="sale.menu_reporting" id="menu_sale_shop_detailed_cds" action="wizard_print_sale_shop_detailed_cds"/>


        <!-- REPORT AND WIZARD OF INVOICE SALES -->
        <record model="ir.ui.view" id="form_view_invoice_values_cdst_start">
            <field name="model">sale.invoice_values_cdst.start</field>
            <field name="type">form</field>
            <field name="name">form_invoice_values_cdst_start</field>
        </record>

        <record model="ir.action.wizard" id="wizard_invoice_values_cdst">
            <field name="name">Sale Invoices Report</field>
            <field name="wiz_name">sale.invoice_values_cdst</field>
        </record>

        <record model="ir.action.report" id="report_invoice_values_cdst">
            <field name="name">Sale Invoices Report</field>
            <field name="model"></field>
            <field name="report_name">sale.invoice_values_cdst.report</field>
            <field name="report">sale_cdst/invoice_sales.fods</field>
            <field name="template_extension">ods</field>
            <field name="translatable">False</field>
        </record>

        <menuitem parent="sale.menu_reporting" id="menu_invoice_values_cdst" icon="tryton-print" action="wizard_invoice_values_cdst" />

    </data>
</tryton>
