<tryton>
    <data>
        <!--Campo para depreciacion acomulada-->
        <record model="ir.ui.view" id="asset_view_tree">
            <field name="model">account.asset</field>
            <field name="inherit" ref="account_asset.asset_view_tree"/>
            <field name="name">asset_tree</field>
        </record>

        <record model="ir.ui.view" id="balance_stock_start_view_form">
            <field name="model">account.fiscalyear.balance_stock.start</field>
            <field name="type">form</field>
            <field name="name">balance_stock_start_form</field>
        </record>

        <record model="ir.action.wizard" id="act_balance_stock">
            <field name="name">Balance Stock</field>
            <field name="wiz_name">account.fiscalyear.balance_stock</field>
        </record>

        <menuitem parent="account.menu_processing" sequence="10" action="act_balance_stock" id="menu_balance_stock"/>

        <!-- Print Auxiliary Book -->
        <record model="ir.ui.view" id="print_auxiliary_book_start_view_form">
            <field name="model">account_col.auxiliary_book_cds.start</field>
            <field name="type">form</field>
            <field name="name">print_auxiliary_book_start_view_form</field>
        </record>

        <record model="ir.action.wizard" id="wizard_auxiliary_book_print">
            <field name="name">Print Auxiliary Book</field>
            <field name="wiz_name">account_col.auxiliary_book_print</field>
        </record>

        <record model="ir.action.report" id="auxiliary_book_print">
            <field name="name">Auxiliary Book</field>
            <field name="model"></field>
            <field name="report_name">account_col.auxiliary_book_cds.report</field>
            <field name="report">conector/reports/auxiliary_book.fods</field>
            <field name="template_extension">ods</field>
            <field name="translatable">False</field>
        </record>
        <menuitem parent="account.menu_reporting" action="wizard_auxiliary_book_print" id="menu_auxiliary_book_print" icon="tryton-print"/>


        <!--Forzar borrado activos-->
        <record model="ir.action.wizard" id="act_invoice_asset_force_draft">
            <field name="name">Asset Force Draft</field>
            <field name="wiz_name">account.invoice_asset.force_draft</field>
        </record>
        <record model="ir.action.keyword" id="action_invoice_asset_force_draft_keyword">
            <field name="keyword">form_action</field>
            <field name="model">account.asset,-1</field>
            <field name="action" ref="act_invoice_asset_force_draft"/>
        </record>


        <!-- Print comprehensive income statement -->
        <record model="ir.action.wizard" id="wizard_income_statement">
            <field name="name">Income statement Report</field>
            <field name="wiz_name">account.income_statement_wizard_cds</field>
        </record>

        <record model="ir.action.keyword" id="act_income_statement_keyword">
            <field name="keyword">form_action</field>
            <field name="model">account.account.type,-1</field>
            <field name="action" ref="wizard_income_statement"/>
        </record>

        <record model="ir.ui.view" id="detailed_income_statement_form">
            <field name="model">account.income_statement.start</field>
            <field name="type">form</field>
            <field name="name">detailed_income_statement_form</field>
        </record>

        <record model="ir.action.report" id="print_income_statement">
            <field name="name">Income statement Report</field>
            <field name="model">account.account.type</field>
            <field name="report_name">account.income_statement_report</field>
            <field name="report">conector/reports/analytic_accounts.fods</field>
            <field name="template_extension">ods</field>
            <field name="translatable">False</field>
        </record>

        <!-- Withholding report -->

        <record model="ir.ui.view" id="print_party_withholding_start_view_form">
            <field name="model">account.party_withholding.start</field>
            <field name="inherit" ref="account_col.print_party_withholding_start_view_form"/>
            <field name="name">print_party_withholding_start_form</field>
        </record>

    </data>
</tryton>
